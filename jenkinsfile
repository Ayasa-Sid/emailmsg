pipeline{
    agent any

    stages{
        stage('compiler stage'){
            step{
            withMaven(maven:'maven_3_8_6'){
            sh 'mvn clean compiler'
              }
            }

        }
        stage('Testing stage'){
           step{
           withMaven(maven:'maven_3_8_5'){
           sh 'mvn test'
             }

           }
        }
        stage('Deployment Stage'){
            step{
            withMaven(maven:'maven_3_8_4'){
            sh 'mvn deploy'

               }
            }


        }




}