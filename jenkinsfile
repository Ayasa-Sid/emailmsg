pipeline{
    agent any
    tools{ maven 'maven_3_8_6' }
              stages{

                     stage('Build maven'){
                         steps{
                          checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Ayasa-Sid/emailmsg.git']]])
                            sh 'mvn clean install'
                           }

                     }


              }


}
